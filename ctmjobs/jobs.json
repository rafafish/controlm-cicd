{
	"Defaults": {
		"Application": "DevOpsLifecycle",
		"SubApplication": "FileTransform",
		"RunAs": "usrcontrolm",
		"Host": "srvcontrolm04"
	},
	"TRANSFORM": {
		"Type": "Folder",
		"Comment": "Code reviewed by dev10 team",
		"DOWNLOAD": {
			"Type": "Job:FileTransfer",
			"ConnectionProfileSrc": "SFTPConn",
			"ConnectionProfileDest": "LocalConn",
			"FileTransfers": [{
				"Src": "~/Adesao/adesao_seguro.csv",
				"Dest": "/home/ctmagent/files/in/",
				"TransferOption": "SrcToDest",
				"TransferType": "Binary"
			}]
		},
		"TRANSFORM": {
			"Type": "Job:Command",
			"Command": "python /home/ctmagent/transform.py /home/ctmagent/files/in/adesao_seguro.csv"
		},
		"VALIDATE": {
			"Type": "Job:Command",
			"Command": "python /home/ctmagent/validate.py /home/ctmagent/files/in/adesao_seguro.csv /home/ctmagent/files/out/adesao_seguro.json"
		},
		"UPLOAD": {
			"Type": "Job:FileTransfer",
			"ConnectionProfileSrc": "LocalConn",
			"ConnectionProfileDest": "SFTPExternal",
			"FileTransfers": [{
				"Src": "/home/ctmagent/files/out/adesao_seguro.json",
				"Dest": "~/Saida/adesao_seguro.json",
				"TransferOption": "SrcToDest",
				"TransferType": "Binary"
			}]
		},
		"Flow": {
			"Type": "Flow",
			"Sequence": ["DOWNLOAD",
			"TRANSFORM",
			"VALIDATE",
			"UPLOAD"]
		}
	}
}